$as_echo "$OSDMODNAME" >&6; }
		OSDADDON="subdir-m += $OSDMODNAME"
	else
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: can't link" >&5
$as_echo "can't link" >&6; }
		OSDADDON=
	fi
