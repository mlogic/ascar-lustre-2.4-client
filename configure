{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for buildid" >&5
$as_echo_n "checking for buildid... " >&6; }
BUILDID=""
if git branch >/dev/null 2>&1; then
	ffw=0
	hash=""
	ver=$(git describe --match v[0-9]_*_[0-9]* --tags)
	if [[ $ver = *-*-* ]]; then
		hash=${ver##*-}
		ffw=${ver#*-}
		ffw=${ffw%-*}
		ver=${ver%%-*}
	fi
	# it's tempting to use [[ $ver =~ ^v([0-9]+_)+([0-9]+|RC[0-9]+)$ ]]
	# here but the portability of the regex on the right is dismal
	# (thanx suse)
	if echo "$ver" | egrep -q "^v([0-9]+_)+([0-9]+|RC[0-9]+)$"; then
		ver=$(echo $ver | sed -e 's/^v\(.*\)/\1/' \
				      -e 's/_RC[0-9].*$//' -e 's/_/./g')
	fi

	# a "lustre fix" value of .0 should be truncated
	if [[ $ver = *.*.*.0 ]]; then
		ver=${ver%.0}
	fi
	# ditto for a "lustre fix" value of _0
	if [[ $ver = v*_*_*_0 ]]; then
		ver=${ver%_0}
	fi
	if [[ $ver = v*_*_* ]]; then
		ver=${ver#v}
		ver=${ver//_/.}
	fi

	# only do this test for lustre (not ldiskfs)
	if test "$PACKAGE" = "lustre" -a "$ver" != "$VERSION"; then
		{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: most recent tag found: $ver does not match current version $VERSION." >&5
$as_echo "$as_me: WARNING: most recent tag found: $ver does not match current version $VERSION." >&2;}
	fi

	if test "$ffw" != "0"; then
		BUILDID="$hash"
		msg="$BUILDID (ahead by $ffw commits)"
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $msg" >&5
$as_echo "$msg" >&6; }
	else
		{ $as_echo "$as_me:${as_lineno-$LINENO}: result: none... congratulations, you must be on a tag" >&5
$as_echo "none... congratulations, you must be on a tag" >&6; }
	fi
elif test -f META; then
	BUILDID=$(sed -ne '/^BUILDID =/s/.*= *//p' META)
	msg="$BUILDID (from META file)"
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $msg" >&5
$as_echo "$msg" >&6; }
else
	{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: FIXME: I don't know how to deal with source trees outside of git that don't have a META file.  Not setting a buildid." >&5
$as_echo "$as_me: WARNING: FIXME: I don't know how to deal with source trees outside of git that don't have a META file.  Not setting a buildid." >&2;}
fi




