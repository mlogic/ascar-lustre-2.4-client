{ $as_echo "$as_me:${as_lineno-$LINENO}: checking if this distro uses dpkg" >&5
$as_echo_n "checking if this distro uses dpkg... " >&6; }
case `lsb_release -i -s 2>/dev/null` in
        Ubuntu | Debian)
                { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
