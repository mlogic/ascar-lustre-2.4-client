MODULES := lquota

quota-objs := lproc_quota.o lquota_lib.o lquota_disk.o lquota_entry.o

qmt-objs := qmt_dev.o qmt_handler.o qmt_lock.o qmt_entry.o qmt_pool.o

qsd-objs := qsd_lib.o qsd_request.o qsd_entry.o qsd_lock.o
